<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_filename"
        android:supportsRtl="true"
        android:theme="@style/Theme.Gwent">
        <activity
            android:filename=".MainActivity"
            android:exported="true"
            android:screenOrientation="landscape">
            <intent-filter>
                <action android:filename="android.intent.action.MAIN" />

                <category android:filename="android.intent.category.LAUNCHER" />
            </intent-filter>

            <meta-data
                android:filename="android.app.lib_filename"
                android:value="" />
        </activity>
    </application>
</manifest>